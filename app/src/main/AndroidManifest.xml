<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zpproject">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.VolleyActivity"
            />
        <activity android:name=".activity.JavaSocketActivity"
            />
        <activity android:name=".activity.HYGroupActivity"
            />
        <activity android:name=".activity.YoutubeActivity"
            />
        <activity android:name=".activity.HoodleActivity"
            />
    </application>

</manifest>